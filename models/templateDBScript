CREATE TABLE `aggregator`.`ac_TB` (
  `acID` INT NOT NULL AUTO_INCREMENT,
  `aircraftTypeID` INT NOT NULL,
  `uicID` INT NOT NULL,
  `acTail` VARCHAR(45) NOT NULL,
  `acMigDate` DATETIME NULL,
  `acCrewChief` VARCHAR(45) NULL,
  `acHours` VARCHAR(45) NULL,
  `acHoursPhaseDue` VARCHAR(45) NULL,
  `acRemarks` VARCHAR(45) NULL,
  `acOpenWOs` VARCHAR(45) NULL,
  `ac30MCrate` VARCHAR(45) NULL,
  `acASDStatus` VARCHAR(45) NULL,
  `acACNStatus` VARCHAR(45) NULL,
  `updateNumber` INT NULL,
  `date` DATETIME NOT NULL,
  PRIMARY KEY (`acID`));